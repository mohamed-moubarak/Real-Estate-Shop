<p id="notice"><%= notice %></p>

<h1>Listing Properties</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Street</th>
      <th>Area</th>
      <th>City</th>
      <th>Building no</th>
      <th>Apartement no</th>
      <th>Famous site</th>
      <th>Surface area</th>
      <th>Price from</th>
      <th>Price to</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.description %></td>
        <td><%= property.street %></td>
        <td><%= property.area %></td>
        <td><%= property.city %></td>
        <td><%= property.building_no %></td>
        <td><%= property.apartement_no %></td>
        <td><%= property.famous_site %></td>
        <td><%= property.surface_area %></td>
        <td><%= property.price_from %></td>
        <td><%= property.price_to %></td>
        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
        <td><%= link_to 'Destroy', property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property', new_property_path %>
